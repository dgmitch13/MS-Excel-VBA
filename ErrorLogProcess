Sub ErrorLogProcess()
'This macro finds flat file created by _error.vbs and populates the table
'Macro recorded 1/8/2007 by Don Mitchelll
'change name of macro from  Errorlog_process to Errorlog

'Diminsion area-----------------
    Dim rCriterial1 As Range
    Dim rCriterial2 As Range
    Dim rCriterial3 As Range
    Dim rCriterial4 As Range
    Dim rEvaluate As Variant
    Dim rEvaluate2
    Dim lcount1
    Dim lcount2
    Dim Wk As Workbook
    Dim sBoxResult
    Dim fname
'-------------------------------

filetoopen = Application _
    .GetOpenFilename("Text Files (*.txt), *.txt")
If filetoopen <> False Then
    MsgBox "Open " & filetoopen
    End If

'-----
'write a error check for file opening
'-----

If filetoopen = False Then
    MsgBox ("You selected cancel, application will close now")
Application.Quit

          
End If


Workbooks.OpenText filename:=filetoopen, StartRow:=1, _
DataType:=xlDelimited, Tab:=True

'Moves the data from a1 to e2 to continue formating and processing

 Range("A1").Select
 Range(Selection, ActiveCell.SpecialCells(xlLastCell)).Select
 Application.CutCopyMode = False
 Selection.Cut
 Range("E2").Select
 ActiveSheet.Paste

'Save spreadsheet as "Integration Error Log Report1_MMDDYY.xls"

 ' Sub AddSaveAsNewWorkbook()
 '   Dim Wk As Workbook
  '  Set Wk = Workbooks.Add
  '  Application.DisplayAlerts = False
  '  Wk.SaveAs Filename:="D:/MyData/SalesData.xls" & Date
 ' End Sub




'Name the tab "Hourly Error"
ActiveSheet.Name = "Hourly Errors"
Worksheets("Hourly Errors").Activate

Cells.Select
'Format the cell with the font, color
        With Selection.Font
        .Name = "Arial"
        .Size = 8
        .Strikethrough = False
        .Superscript = False
        .Subscript = False
        .OutlineFont = False
        .Shadow = False
        .Underline = xlUnderlineStyleNone
        .ColorIndex = xlAutomatic
    End With
        
    Rows("1:1").Select
        With Selection.Interior
        .ColorIndex = 43
        .Pattern = xlSolid
    End With

       
    Range("A1").Select
    ActiveCell.FormulaR1C1 = "Source"
    Range("B1").Select
    ActiveCell.FormulaR1C1 = "Reason"
    Range("C1").Select
    ActiveCell.FormulaR1C1 = "Action Required By"
    Range("D1").Select
    ActiveCell.FormulaR1C1 = "Required Resolution"
    Range("E1").Select
    ActiveCell.FormulaR1C1 = "Error Log Date/Time(EST)"
    Range("F1").Select
    ActiveCell.FormulaR1C1 = "Candidate"
    Range("G1").Select
    ActiveCell.FormulaR1C1 = "Requisition"
    Range("H1").Select
    ActiveCell.FormulaR1C1 = "Step"
    Range("I1").Select
    ActiveCell.FormulaR1C1 = "Status"
    Range("J1").Select
    ActiveCell.FormulaR1C1 = "PAN Step"
    Range("K1").Select
    ActiveCell.FormulaR1C1 = "Pan Status"
    Range("L1").Select
    ActiveCell.FormulaR1C1 = "Error"
           
    Selection.RowHeight = 50
    Rows("1:1").Select
    Selection.Font.Bold = True
        
    Selection.AutoFilter
    Columns("A:A").EntireColumn.AutoFit
    
'This phase of the macro creates the second page for "Purge Report File"

' CreatePurgeTab Macro
' Macro recorded 1/5/2007 by Don Mitchelll
'
'Create New Sheet
    'Sheets.Add
    
    Sheets.Add after:=Sheets(Sheets.count)  '-- place at end
    
    'Sheets.Add Before:=ActiveSheet          '-- default
    'Sheets.Add After:=ActiveSheet
    'Rename current Sheet
     
     ActiveSheet.Name = "Nightly Purge Error"
     
    'ActiveSheet.Name = "D" & Format(Date, "yyyymmdd")
    'ActiveSheet.Name = "D" & Format(Range("a1"), "yyyymmdd")
    'ActiveSheet.Name = "D" & Format(Range("a1"), "yyyy_mmdd_hhmm")
    'Create New Sheet and name it NewSheet
    'ActiveWorkbook.Worksheets.Add(After:=ActiveSheet).Name = "NewSheet"
    
    Worksheets("Nightly Purge Error").Activate

    Rows("1:1").Select
    With Selection.Interior
        .ColorIndex = 43
        .Pattern = xlSolid
    End With
    Range("A1").Select
    ActiveCell.FormulaR1C1 = "Candidate"
    Range("B1").Select
    ActiveCell.FormulaR1C1 = "Test Date"
    Range("C1").Select
    ActiveCell.FormulaR1C1 = "PAN error message"
    Columns("B:B").Select
    Columns("A:A").EntireColumn.AutoFit
    Columns("C:C").EntireColumn.AutoFit
    Rows("1:1").Select
    Selection.Font.Bold = True
    With Selection.Font
        .Name = "Arial"
        .Size = 9
        .Strikethrough = False
        .Superscript = False
        .Subscript = False
        .OutlineFont = False
        .Shadow = False
        .Underline = xlUnderlineStyleNone
        .ColorIndex = xlAutomatic
    End With
        Range("D13").Select


' This macro handles the first 4 deletion steps in the spreadsheet
' DelStep Macro
' Macro recorded 1/9/2007 by Don Mitchelll
' Macro updated 1/18/2007 by Don Mitchell
'

Sheets("Hourly Errors").Select
ActiveSheet.Name = "Hourly Errors"
Worksheets("Hourly Errors").Activate



'Step 1 : Verified and working correctly
    Selection.AutoFilter Field:=6, Criteria1:="="
    Range("A2").Select
    Range(Selection, ActiveCell.SpecialCells(xlLastCell)).Select
    Selection.ClearContents
    'add delete up to get rid of blanks
    Selection.Delete Shift:=xlUp
    'Selection.EntireRow.Delete
    
    Selection.AutoFilter Field:=6
  
 'Step 2 : Verified and working correctly
    Selection.AutoFilter Field:=8, Criteria1:="No. of Records: 0"
    Range("A2").Select
    Range(Selection, ActiveCell.SpecialCells(xlLastCell)).Select
    Selection.ClearContents
    Selection.AutoFilter Field:=8
 
'Step 3 : Verified and working correctly
    Selection.AutoFilter Field:=8, Criteria1:="PHASE_II"
    Set rCriterial1 = Cells.Columns(8).Find(What:="Phase_II")
    rEvaluate = TypeName(rCriterial1)
    
    If rEvaluate = "Nothing" Then
        Selection.AutoFilter Field:=8
    Else
        Selection.AutoFilter Field:=9, Criteria1:="HOLD"
        Set rCriterial2 = Cells.Columns(9).Find(What:="HOLD")
        rEvaluate = TypeName(rCriterial2)
            
            If rEvaluate = "Nothing" Then
                Selection.AutoFilter Field:=9
            Else
                Selection.AutoFilter Field:=10, Criteria1:="PHASE_II"
                Set rCriterial3 = Cells.Columns(10).Find(What:="PHASE_II")
                rEvaluate = TypeName(rCriterial3)
                
                      If rEvaluate = "Nothing" Then
                        Selection.AutoFilter Field:=10
                      Else
                        Selection.AutoFilter Field:=11, Criteria1:="HOLD"
                        Set rCriterial4 = Cells.Columns(11).Find(What:="HOLD")
                        rEvaluate = TypeName(rCriterial4)
                        
                                If rEvaluate = "Nothing" Then
                                  Selection.AutoFilter Field:=11
                                Else
                                                    
                                  Selection.ClearContents
  
    End If
            End If
                      End If
                              End If
                          
         'Reset all autofilters for accurate following operations
  
                    Selection.AutoFilter Field:=8
                    Selection.AutoFilter Field:=9
                    Selection.AutoFilter Field:=10
                    Selection.AutoFilter Field:=11
  'Step 4 : Verified and working correctly
    
    Selection.AutoFilter Field:=8, Criteria1:="NEW"
    Set rCriterial1 = Cells.Columns(8).Find(What:="NEW")
    rEvaluate = TypeName(rCriterial1)
    
    If rEvaluate = "Nothing" Then
        Selection.AutoFilter Field:=8
    Else
       Selection.AutoFilter Field:=9, Criteria1:="DUPLICATE AIRPORT OR APPLICATION"
       Set rCriterial2 = Cells.Columns(9).Find(What:="DUPLICATE AIRPORT OR APPLICATION")
       rEvaluate = TypeName(rCriterial2)
       
            If rEvaluate = "Nothing" Then
                Selection.AutoFilter Field:=9
            Else
                Selection.ClearContents
                                               
    End If
            End If
                    
    
    
    'Reset all autofilters for accurate following operations
  
                    Selection.AutoFilter Field:=8
                    Selection.AutoFilter Field:=9
                    Selection.AutoFilter Field:=10
                    Selection.AutoFilter Field:=11
    
                'This brings the deletion section to an end.
    
    
' This macro handles the reminder update steps in the spreadsheet
' UpdateStep Macro
' Macro recorded 1/12/2007 by Don Mitchelll
' Macro updated 1/18/2007 by Don Mitchell
'

lcount = 0
rEvaluate2 = 1

'Range("A2").Select ' maybe overkill but want to make sure cursor in right place

'Cleanup spreadsheet of "Blanks" and then set filters to "ALL" and sort data

    Selection.AutoFilter Field:=8, Criteria1:="="
    Selection.AutoFilter Field:=9, Criteria1:="="
    Selection.AutoFilter Field:=10, Criteria1:="="
    Selection.AutoFilter Field:=11, Criteria1:="="

    Range("A2").Select
    Range(Selection, ActiveCell.SpecialCells(xlLastCell)).Select
    Selection.ClearContents
   
    Selection.Delete Shift:=xlUp
    'Selection.EntireRow.Delete
        
        Selection.AutoFilter Field:=1
        Selection.AutoFilter Field:=2
        Selection.AutoFilter Field:=3
        Selection.AutoFilter Field:=4
        Selection.AutoFilter Field:=5
        Selection.AutoFilter Field:=6
        Selection.AutoFilter Field:=7
        Selection.AutoFilter Field:=8
        Selection.AutoFilter Field:=9
        Selection.AutoFilter Field:=10
        Selection.AutoFilter Field:=11

        Selection.Sort Key1:=Range("E2"), Order1:=xlAscending, Key2:=Range("e2") _
        , Order2:=xlAscending, Key3:=Range("G2"), Order3:=xlAscending, Header _
        :=xlGuess, OrderCustom:=1, MatchCase:=False, Orientation:=xlTopToBottom _
        , DataOption1:=xlSortNormal, DataOption2:=xlSortNormal, DataOption3:= _
        xlSortNormal

